+}
+
+.edit-config-btn {
+  background: #e0f2fe;
+  border-color: #81d4fa;
+  color: #0277bd;
+}
+
+.edit-config-btn:hover {
+  background: #b3e5fc;
+}
+
+.delete-config-btn {
+  background: #ffebee;
+  border-color: #ffcdd2;
+  color: #c62828;
+}
+
+.delete-config-btn:hover {
+  background: #ffcdd2;
+}
+
+.config-details {
+  margin-top: 12px;
+}
+
